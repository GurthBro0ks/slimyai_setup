{
  "name": "slimyai_setup",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "index.js",
  "workspaces": [
    "admin-ui",
    "admin-api",
    "packages/core"
  ],
  "scripts": {
    "start": "node index.js",
    "refresh:commands": "node scripts/refresh-commands.js",
    "restart:bot": "node scripts/restart-bot.js",
    "test:slash": "node scripts/run-slash-tests.js",
    "qa:full": "npm run refresh:commands && npm run restart:bot && node scripts/run-slash-tests.js",
    "admin:ui:build": "npm --workspace admin-ui run build",
    "admin:ui:start": "npm --workspace admin-ui run start",
    "admin:bootstrap": "bash ./scripts/bootstrap-admin.sh",
    "admin:api:deps": "npm -w admin-api i express helmet cors cookie-parser express-rate-limit dotenv jsonwebtoken compression morgan mysql2 undici",
    "admin:api:restart": "sudo systemctl restart admin-api",
    "admin:api:doctor": "node admin-api/scripts/doctor.js"
  },
  "dependencies": {
    "@discordjs/rest": "^2.6.0",
    "@google-cloud/vision": "^5.3.4",
    "cheerio": "^1.1.0",
    "discord.js": "^14.23.2",
    "googleapis": "^164.1.0",
    "luxon": "^3.7.2",
    "mysql2": "^3.15.3",
    "openai": "^6.7.0",
    "proper-lockfile": "^4.1.2",
    "uuid": "^13.0.0"
  }
}
