{
  "name": "slimyai_setup",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "node deploy-commands.js",
    "check:conflicts": "git ls-files | grep -E '\\\\.(js|ts)$' | grep -v -E 'node_modules|\\\\.bak\\\\.|\\\\.broken\\\\.' | xargs grep -nE '^(<<<<<<<|=======|>>>>>>>)' && exit 1 || echo \"no conflict markers\"",
    "preflight": "npm run check:conflicts && node supersnail-costs.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "better-sqlite3": "^11.5.0",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.3",
    "googleapis": "^133.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.57.0",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "tsx": "^4.20.6"
  }
}
